{{!
    Home page (not connected)
}}

{{! hack to hide main_content add jjupin }}
<div class="d-none">
    <section class="d-none" style="display:none;">
        {{{ output.main_content }}}
    </section>
</div>
{{! END hack to hide main_content add jjupin }}

{{> theme_boost/head }}


<body {{{bodyattributes}}} >
    <div id="page-wrapper" class="page-wrapper-home-mineticeboost">

        {{{ output.standard_top_of_body_html }}}

        <div id="page" class="container-fluid home-mineticeboost">

            {{! home page header mineticeboost  add jjupin }}
            {{> theme_mineticeboost/home_header }}

            <div id="page-content" class="row pb-3">
                <div id="region-main-box" class="col-12">
                    <section id="region-main" class="d-flex justify-content-center">


                        {{! home page mineticeboost  add jjupin }}


                        {{#isguestuser}} {{> theme_mineticeboost/home_guest }} {{/isguestuser}}

                        {{^isguestuser}} {{> theme_mineticeboost/home_content }} {{/isguestuser}}


                            {{! END home pagemineticeboost  add jjupin }}


                    </section>
                </div>
            </div>
        </div>

        {{{ output.standard_after_main_region_html }}}
        {{> theme_boost/footer }}


    </div>
    {{{ output.standard_end_of_body_html }}}

</body>

</html>
{{#js}}
    require(['theme_boost/loader']);
{{/js}}
